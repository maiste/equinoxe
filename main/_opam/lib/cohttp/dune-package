(lang dune 2.9)
(name cohttp)
(version 5.0.0)
(library
 (name cohttp)
 (kind normal)
 (synopsis "Co-operative Client/Server HTTP library.")
 (archives (byte cohttp.cma) (native cohttp.cmxa))
 (plugins (byte cohttp.cma) (native cohttp.cmxs))
